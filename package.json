{
  "name": "svelte-app",
  "description": "Svelte with TypeScript example with component testing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "update:badges": "update-badge cypress cypress-svelte-unit-test",
    "test": "cypress-expect run --passing 2",
    "test:local": "start-test 5000 test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "12.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-typescript": "4.1.2",
    "@tsconfig/svelte": "1.0.3",
    "cypress": "5.3.0",
    "cypress-expect": "2.1.2",
    "cypress-svelte-unit-test": "3.3.1",
    "dependency-version-badge": "1.3.0",
    "rollup": "2.30.0",
    "rollup-plugin-livereload": "1.3.0",
    "rollup-plugin-svelte": "5.2.3",
    "rollup-plugin-terser": "5.3.0",
    "start-server-and-test": "1.11.5",
    "svelte": "3.24.0",
    "svelte-check": "0.1.55",
    "svelte-preprocess": "4.0.8",
    "tslib": "^2.0.0",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
